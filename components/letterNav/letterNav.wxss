.letter-nav{
    width: 60rpx;
    flex-direction: column;
    -webkit-display:flex;
    display:flex;
    -webkit-align-items:center;
    align-items:center;
    -webkit-justify-content:center;
    justify-content:center;
    position: fixed; 
    z-index: 99;
}
.letter-item{
    flex: 1;
    display: block;
    font-size: 22rpx;
    color: #1883FE;
    text-align: center;
    width: 100%;
    height: 36rpx;
}
.icon-search {
    width: 22rpx;
    height: 24rpx;
    margin-bottom: 8rpx;
}